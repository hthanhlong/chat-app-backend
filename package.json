{
  "name": "chat_page_backend",
  "version": "2.0.1",
  "description": "backend for chat page",
  "main": "server.js",
  "scripts": {
    "production": "npm run build && node --no-deprecation dist/server.js",
    "build": "npm run pre-build && npm run build-ts",
    "dev": "nodemon --ext ts --exec ts-node src/server.ts",
    "watch-node": "nodemon -r dist/server.js",
    "pre-build": "rimraf ./dist",
    "clean-up": "rimraf ./dist && rimraf ./logs && rimraf ./node_modules && rimraf ./package-lock.json && rimraf ./yarn.lock",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "check": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "google-auth-library": "^9.7.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^11.8.0",
    "punycode": "^2.3.1",
    "qs": "^6.11.2",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/newrelic": "^9.14.3",
    "@types/newrelic__winston-enricher": "^0.1.5",
    "@types/node": "^20.10.6",
    "@types/redis": "^4.0.11",
    "@types/request": "^2.48.12",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
